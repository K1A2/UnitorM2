<?xml version="1.0" encoding="utf-8"?>
<unitor.uni.k1a2.unitor.CheckView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent" android:padding="5dp">

    <ImageView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:layout_weight="0.1"
        android:layout_marginRight="5dp"
        android:id="@+id/FragDial_file_Image"
        android:src="@drawable/ic_music_note_white_48dp"/>

    <TextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:id="@+id/FragDial_file_name"
        android:layout_weight="1"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:textSize="0sp"
        android:text="ddddddd"
        android:id="@+id/FragDial_file_path"
        android:visibility="invisible"/>

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:id="@+id/FragDial_list_Check"
        android:focusable="false"
        android:clickable="false"/>

</unitor.uni.k1a2.unitor.CheckView>